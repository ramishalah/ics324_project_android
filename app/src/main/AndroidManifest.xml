        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.Option1Activity" />
        <activity android:name=".Activities.Option2Activity" />
        <activity android:name=".Activities.Option5Activity"></activity>
        <activity android:name=".Activities.Option6Activity">

        </activity>
    </application>

</manifest>